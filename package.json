{
  "name": "nodejs",
  "version": "1.0.3",
  "description": "NodeJs training",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "babel tasks -d dist",
    "task1-1": "nodemon ./tasks/app1-1.js",
    "task1-2": "nodemon ./tasks/app1-2.js",
    "task1-3": "nodemon --exec babel-node ./tasks/app1-3.js",
    "task1-1b": "npm run build && nodemon ./dist/app1-1.js",
    "task2-1": "nodemon --experimental-modules --inspect ./tasks/app2-1.js",
    "task2-1b": "npm run build && nodemon --inspect ./dist/app2-1.js",
    "checkDist": "npm run build && eslint dist/** -c dist/.eslintrc",
    "check": "eslint tasks/**",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@hapi/joi": "^17.1.0",
    "babel-eslint": "^10.1.0",
    "csvtojson": "^2.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-node": "^11.0.0",
    "express": "^4.17.1",
    "express-joi-validation": "^4.0.3",
    "nodemon": "^2.0.2",
    "winston": "^3.2.1"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.0",
    "cors": "^2.8.5",
    "express-joi-validation": "^4.0.3",
    "express-winston": "^4.0.3",
    "jsonwebtoken": "^8.5.1",
    "uuid": "^7.0.1"
  }
}
